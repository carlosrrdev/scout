<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>


<?import javafx.scene.control.ListView?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="codes.crr.scout.ListController" VBox.vgrow="ALWAYS"
      xmlns="http://javafx.com/javafx">
    <HBox HBox.hgrow="ALWAYS">
        <Button text="Load SQL file"/>
        <Button text="Scout new directory"/>
        <HBox HBox.hgrow="ALWAYS" alignment="TOP_RIGHT">
            <TextField/>
            <Button text="Search"/>
        </HBox>
    </HBox>
    <VBox VBox.vgrow="ALWAYS">
        <ListView fx:id="myList" VBox.vgrow="ALWAYS"/>
    </VBox>
</VBox>